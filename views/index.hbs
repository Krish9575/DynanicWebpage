  {{!-- banner section -1 --}}

  <section id="banner1_section" class="background1 py-5">

    <div class="container text-center">
      <div class="row">
        <div class="col-md-12">
          <h2>
            Start learning new technology here!!
          </h2>

          <p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Optio, expedita omnis consequuntur quibusdam amet
