        </div>
      </div>
    </div>
  </section>


  {{!-- services section --}}

  <section id="services_section" class="py-4">

    <div class="container">

      <h2 class="text-center text-primary">
        Services that we provide
      </h2>

      <div class="row mt-4">

        {{!-- first services col --}}

        {{#each services}}
        <div class="col-md-4">

          <div class="card">
            <div class="card-body">
              <div class="container text-center ">
                <i class="{{this.icon}} services_icon  text-secondary"></i>
              </div>

              <h5>{{this.title}}</h5>
              <p>
                {{this.description}}

              </p>

              <a href="{{this.link}}">{{this.linkText}}</a>
              <a href="#" class="ml-5">Support</a>

            </div>

          </div>

        </div>

        {{/each}}

      </div>


    </div>

  </section>

  {{!-- banner section-2 --}}

  <section class="banner_section2 py-5">


    <div class="container">
      <div class="row">
