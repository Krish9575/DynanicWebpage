  {{!-- contact form --}}

  <section id="contact_us_section" class="py-5 background1">
    <div class="container">
      <div class="row">
        <h2 class="text-center text-muted">Contact Us</h2>
        <div class="col-md-12">

          <form action="/process-contact-form" method="post">


            <div class="mb-3">
              <label for="exampleInputEmail1" class="form-label">Email address</label>
              <input name="email" type="email" class="form-control" id="exampleInputEmail1"
                aria-describedby="emailHelp">
              <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
            </div>


            <div class="mb-3">
              <label for="contactNumberField" class="form-label">Your Contact number</label>
              <input name=" phone" type="number" class="form-control" id="exampleInputPassword1">
            </div>


            <div class="form-floating my-2">
              <textarea name="query" style=" height:250px" class="form-control" placeholder="Leave a comment here"
                id="floatingTextarea"></textarea>
              <label for="floatingTextarea">your query</label>
            </div>

            <div class="container text-center">
              <button type="submit" class="btn btn-dark">Submit</button>
            </div>


          </form>
        </div>
      </div>
    </div>
  </section>

